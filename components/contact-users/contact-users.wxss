.weui-search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.return {
  margin-right: 10px;
  line-height: 28px;
  color: #09bb07;
